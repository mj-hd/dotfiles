{
	"coc.preferences.snippets.enable": false,
	"coc.preferences.formatOnSaveFiletypes": [
		"css",
		"Markdown",
		"dart",
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact",
		"rust"
	],
	"coc.preferences.semanticTokensHighlights": false,
	"diagnostic.refreshOnInsertMode": true,
	"diagnostic.refreshAfterSave": true,

	"explorer.floating.border.enable": false,
	"explorer.icon.enableNerdfont": true,

	"eslint.autoFixOnSave": true,

	"rust-analyzer.enable": true,
	"rust-analyzer.inlayHints.enable": false,

	"flutter.workspaceFolder.ignore": [
		"~/.pub-cache",
		"~/fvm"
 	],
	"flutter.provider.enableSnippet": true,
	"flutter.runDevToolsAtStartup": true,
	"flutter.autoOpenDevLog": true,
	"flutter.autoHideDevLog": true,
	"flutter.selectedDeviceId": "S",
	"dart.analysisExcludedFolders": [
		"~/.pub-cache",
		"~/fvm"
 	],
	"dart.showTodos": false,

	"tsserver.log": "verbose",
	"tsserver.maxTsServerMemory": 16384,

	"go.goplsOptions": {
		"completeUnimported": true
	}
}
